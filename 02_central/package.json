    "@aws-sdk/client-s3": "^3.896.0",
    "@aws-sdk/client-ssm": "^3.896.0",
    "@aws-sdk/lib-dynamodb": "^3.895.0",
    "@octokit/webhooks-types": "^7.6.1",
    "@types/aws-lambda": "^8.10.152",
    "@types/node": "^24.5.2",
    "@typescript-eslint/eslint-plugin": "^8.44.1",
    "@typescript-eslint/parser": "^8.44.0",
    "eslint": "^9.36.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.6.2",
    "rimraf": "^6.0.1",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": "^22"
  },
  "homepage": "https://github.com/Mi3-14159/GitGazer#readme",
  "keywords": [],
  "main": "index.js",
  "name": "api-lambda",
  "optionalDependencies": {
    "@rollup/rollup-darwin-x64": "^4.52.2",
    "@rollup/rollup-linux-x64-gnu": "^4.52.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mi3-14159/GitGazer.git"
  },
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "buildZip": "npm run build && npm run zip",
    "clean": "rimraf ./dist ./tmp",
    "dev": "node --env-file=.env.dev --watch -r ts-node/register src/develop.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "postbuild": "cp package*.json dist && cd dist && npm ci --omit=dev",
    "prebuild": "npm run clean",
    "pretty": "prettier --write src",
    "test:unit": "vitest --project unit --run",
    "zip": "cd dist && zip lambda.zip -qr *"
  },
  "version": "1.0.0"
}
