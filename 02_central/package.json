{
  "author": "",
  "bugs": {
    "url": "https://github.com/Mi3-14159/GitGazer/issues"
  },
  "dependencies": {
    "pino": "^10.1.0"
  },
  "description": "lambda as API",
  "devDependencies": {
    "@aws-sdk/client-apigatewaymanagementapi": "^3.943.0",
    "@aws-sdk/client-cognito-identity-provider": "^3.943.0",
    "@aws-sdk/client-dynamodb": "^3.943.0",
    "@aws-sdk/client-s3": "^3.943.0",
    "@aws-sdk/lib-dynamodb": "^3.943.0",
    "@octokit/webhooks-types": "^7.6.1",
    "@types/aws-lambda": "^8.10.159",
    "@types/node": "^24.10.1",
    "@typescript-eslint/eslint-plugin": "^8.48.1",
    "@typescript-eslint/parser": "^8.45.0",
    "eslint": "^9.39.1",
    "pino-pretty": "^13.1.3",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "ts-node": "^10.9.2",
    "tsc-alias": "^1.8.16",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.9.3",
    "vitest": "^4.0.15"
  },
  "engines": {
    "node": "^24"
  },
  "homepage": "https://github.com/Mi3-14159/GitGazer#readme",
  "keywords": [],
  "main": "index.js",
  "name": "api-lambda",
  "optionalDependencies": {
    "@rollup/rollup-darwin-x64": "^4.53.3",
    "@rollup/rollup-linux-x64-gnu": "^4.53.3"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Mi3-14159/GitGazer.git"
  },
  "scripts": {
    "build:alerting": "npm run prebuild && tsc --project tsconfig.alerting.json && tsc-alias && npm run postbuild",
    "build:api": "npm run prebuild && tsc --project tsconfig.api.json && tsc-alias && npm run postbuild",
    "buildZip:alerting": "npm run build:alerting && npm run zip",
    "buildZip:api": "npm run build:api && npm run zip",
    "clean": "rimraf ./dist ./tmp",
    "dev:api": "node --env-file=.env.dev --watch -r ts-node/register src/develop.ts",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "postbuild": "cp package*.json dist && cd dist && npm ci --omit=dev",
    "prebuild": "npm run clean",
    "pretty": "prettier --write src",
    "test:unit:api": "vitest --project unit:api --run",
    "zip": "cd dist && zip lambda.zip -qr *"
  },
  "version": "1.0.0"
}
